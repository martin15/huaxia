<div class="admin-content">
  <h2>New Principle Note</h2><br />

  <%= form_for([:admin, @principle_note], :html => { :multipart => true }) do |f| %>
    <%= f.error_messages %>
    <%= render :partial => 'form', :locals => { :f => f} %>
    <div class="admin-actions">
      <%= f.submit 'Create Principle Note' %>&nbsp;&nbsp;&nbsp;
      <%= link_to 'Back', admin_principle_notes_path %>
    </div>
  <% end %>
</div>
