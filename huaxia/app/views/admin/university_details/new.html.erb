<div class="col-md-12 content">
  <h2>New University Detail</h2>
  <div class="col-md-12">
    <%= form_for([:admin, @university_detail], 
                  :url => admin_university_university_details_path(@university.id),
                  :html => { :multipart => true, :class => "form-horizontal"}) do |f| %>
      <%= f.error_messages %>
      <%= render :partial => 'form', :locals => { :f => f} %>
      <div class="form-group">
        <div class="col-sm-2">&nbsp;</div>
        <div class="col-sm-10">
          <%= f.submit :class => "btn btn-success" %>
          <%= link_to 'Back', admin_university_university_details_path(@university.id), :class => "btn btn-info" %>
        </div>
      </div>
    <% end %>
  </div>
</div>
