<div class="form-group">
  <%= f.label :name, "Name *", :class => "col-sm-2 control-label" %>
  <div class="col-sm-10">
    <%= f.text_field :name, :class => "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :city, "City *", :class => "col-sm-2 control-label" %>
  <div class="col-sm-10">
    <%= f.text_field :city, :class => "form-control" %>
  </div>
</div>
<div class="form-group margin_b_0">
  <%= f.label :url, "URL *", :class => "col-sm-2 control-label" %>
  <div class="col-sm-10">
    <%= f.text_field :url, :class => "form-control" %>
  </div>
</div>
<div class="form-group">
  <div class="col-sm-2">&nbsp;</div>
  <div class="col-sm-10">
    Please write url with 'http://' e.g 'http://www.huaxia-icec.com'
  </div>
</div>
<div class="form-group margin_b_30">
  <%= f.label :logo, "Logo *", :class => "col-sm-2 control-label" %>
  <div class="col-sm-10">
    <%= f.file_field :logo, :class => "control-label btn btn-default btn-file" %>
  </div>
</div>
