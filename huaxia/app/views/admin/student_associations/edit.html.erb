<div class="col-md-12 content">
  <h2>Edit Student Association</h2>
  <div class="col-md-12">
    <%= form_for([:admin, @student_association],
                  :html => { :multipart => true, :method => :put,  :class => "form-horizontal"}) do |f| %>
      <%= f.error_messages %>
      <div class="form-group margin_b_30">
        <div class="col-sm-2">&nbsp;</div>
        <div class="col-sm-10">
          <%= image_tag @student_association.logo.url(:medium) %>
        </div>
      </div>
      <%= render :partial => 'form', :locals => { :f => f} %>
      <div class="form-group">
        <div class="col-sm-2">&nbsp;</div>
        <div class="col-sm-10">
          <%= f.submit :class => "btn btn-success" %>
          <%= link_to 'Back', admin_student_associations_path, :class => "btn btn-info" %>
        </div>
      </div>
    <% end %>
  </div>
</div>
