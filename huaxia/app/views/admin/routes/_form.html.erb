<%= hidden_field_tag "route[airline_id]", @airline.id %>

<div class="form-group">
  <%= f.label :origin, "Origin *", :class => "col-sm-2 control-label" %>
  <div class="col-sm-10">
    <%= f.text_field :origin, :class => "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :transit, "Transit", :class => "col-sm-2 control-label" %>
  <div class="col-sm-10">
    <%= f.text_field :transit, :class => "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :destination, "Destination *", :class => "col-sm-2 control-label" %>
  <div class="col-sm-10">
    <%= f.text_field :destination, :class => "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :estimation_price, :class => "col-sm-2 control-label" %>
  <div class="col-sm-10">
    <%= f.text_field :estimation_price, :class => "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :estimation_time, :class => "col-sm-2 control-label" %>
  <div class="col-sm-10">
    <%= f.text_field :estimation_time, :class => "form-control" %>
  </div>
</div>
<div class="form-group margin_b_30">
  <%= f.label :note, :class => "col-sm-2 control-label" %>
  <div class="col-sm-10">
    <%= f.text_area :note, :class => "form-control", :rows => "10" %>
  </div>
</div>
